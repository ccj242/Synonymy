<application xmlns="http://ns.adobe.com/air/application/23.0">
    <id>com.jarvisfilms.synonymylite</id>
    <versionNumber>4.0.0</versionNumber>
    <versionLabel>4.0.0</versionLabel>
    <filename>Synonymy Free</filename>
    <description></description>
    <name>Synonymy Free</name>
    <copyright></copyright>
    <initialWindow>
